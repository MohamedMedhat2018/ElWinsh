<?xml version="1.0" encoding="utf-8"?>
<resources>

<style name="MaterialTheme" parent="ParentMaterialTheme">
    <!-- Customise for only Lollipop and Above Versions features. -->

</style>

    <style name="AppTheme.AppBarOverlay" parent="Theme.AppCompat.Light.NoActionBar">
    </style>

    <!--not usage-->
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


    <style name="ToolbarTheme" parent="ThemeOverlay.AppCompat.Light">

        <item name="android:textColorPrimary">@android:color/white
        </item>   <!-- Text color of Toolbar Title -->
        <item name="android:textColorSecondary">@android:color/white</item> <!-- Color of Overflow Menu ( 3 dots icon )
                                                                                    and color of Navigation Drawer Icon-->
    </style>

    <style name="MyEditTextTheme">
        <!-- Used for the bottom line when not selected / focused -->
        <item name="colorControlNormal">@color/secondaryColor</item>
        <item name="android:colorControlActivated">@color/secondaryColor</item>
        <item name="android:colorControlHighlight">@color/colorControlHighlight</item>
        <item name="android:textColorHint">@color/White</item>
        <!-- colorControlActivated & colorControlHighlight use the colorAccent color by default -->
    </style>


</resources>